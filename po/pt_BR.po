#: ../data/tumblefile.desktop.in.in.h:1
msgid "Automatic, chronological file organizer"
msgstr "Um organizador de arquivos cronológico e automático"

#: ../data/tumblefile.desktop.in.in.h:2
msgid "Tumblefile"
msgstr "Tumblefile"

#. C/C++ incompatibility: normal C initialization yields warnings
#: ../src/main.cc:20
msgid "Show version information and exit"
msgstr "Exibir a informação da versão e sair"

#: ../src/main.cc:21
msgid "Run with window minimized to tray"
msgstr "Executar com a janela minimizada na bandeja do sistema"

#: ../src/main-window.cc:21
msgid "Hide"
msgstr "Ocultar"

#: ../src/main-window.cc:85
msgid "Tumblefile is running."
msgstr "Tumblefile está sendo executado."

#: ../src/main-window.cc:87
msgid "It will timestamp and move files from Desktop to Documents every 5 minutes."
msgstr "Ele irá verificar dados de tempo e mover os arquivos da área de trabalho para seus documentos a cada 5 minutos."

#: ../src/tray-icon.cc:36
msgid "Tumblefile – click to hide the window"
msgstr "Tumblefile – clique para ocultar a janela"

#: ../src/tray-icon.cc:40
msgid "Tumblefile – click to show the window"
msgstr "Tumblefile – clique para exibr a janela"

